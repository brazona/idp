{
  "name": "app-ui",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "dev": "npm run env && ng build --watch --configuration development --output-path ../api/target/idp/app",
    "build": "npm run env && ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "clean": "rm -rf ./node_modules package-lock.json .cache dist .angular",
    "reinstall": "npm run clean && npm cache clean --force && npm install --force",
    "env": "cp ./src/assets/env.template.js ./src/assets/env.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.13",
    "@angular/cdk": "^18.2.11",
    "@angular/common": "^18.2.11",
    "@angular/compiler": "^18.2.11",
    "@angular/core": "^18.2.11",
    "@angular/flex-layout": "^15.0.0-beta.42",
    "@angular/forms": "^18.2.11",
    "@angular/material": "^18.2.11",
    "@angular/platform-browser": "^18.2.11",
    "@angular/platform-browser-dynamic": "^18.2.11",
    "@angular/router": "^18.2.11",
    "@material/card": "^14.0.0",
    "@material/density": "^14.0.0",
    "angular-material": "^1.2.5",
    "bootstrap": "^5.3.3",
    "class-transformer": "^0.5.1",
    "materialize-css": "^1.0.0",
    "moment": "^2.30.1",
    "ngx-toastr": "^19.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.8.1",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.11",
    "@angular-devkit/schematics": "^18.2.11",
    "@angular/cli": "~18.2.11",
    "@angular/compiler-cli": "^18.2.11",
    "@types/jasmine": "~5.1.4",
    "jasmine-core": "~5.4.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.5.4"
  }
}
